{
    "name": "lemontree-plugin",
    "description": "LemonTree Plugin with all code and features in Laravel 8",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.0",
        "aws/aws-sdk-php": "^3.0",
        "laravel/ui": "^3.0",
        "laravel/socialite": "^5.0",
        "laravel/passport": "^10.0",
        "laravel/scout": "^10.0",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "laravel/cashier": "^12.0",
        "laravel/nova": "^4.0",
        "laravel/horizon": "^5.0",
        "laravel/echo": "^1.10",
        "laravel/dusk": "^6.0",
        "laravel/sanctum": "^2.0",
        "laravel/fortify": "^1.0",
        "laravel/breeze": "^1.0",
        "laravel/laravel": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0",
        "fzaninotto/faker": "^1.0",
        "symfony/css-selector": "^5.0",
        "symfony/dom-crawler": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    }
}
